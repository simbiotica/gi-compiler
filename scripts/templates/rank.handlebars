<div class="mod-rank-header">
  <div class="wrap">
    <div class="row">
      <div class="grid-12">
        <h1>{{question}}</h1>
      </div>
    </div>
  </div>
</div>

<div class="mod-rank-list">
  <div class="wrap">
    <div class="row">
      <div class="grid-12">
        <ul>
          {{#each answers}}
            <li>
              <strong>{{score}}</strong> {{target}}
            </li>
          {{/each}}
        </ul>
      </div>
    </div>
  </div>
</div>
